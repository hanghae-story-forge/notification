    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "github:fetch": "tsx scripts/fetch-github-projects.ts",
    "github:sync": "tsx scripts/sync-cycles-from-github.ts",
    "github:members": "tsx scripts/fetch-org-members.ts",
    "github:sync-members": "tsx scripts/sync-members.ts",
    "github:assign": "tsx scripts/assign-members-to-generation.ts",
    "github:fetch-comments": "tsx scripts/fetch-issue-comments.ts",
    "github:poll-projects": "tsx scripts/poll-github-projects.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@apollo/sandbox": "^2.7.3",
    "@apollo/server": "^5.2.0",
    "@hono/node-server": "^1.14.1",
    "@hono/zod-openapi": "^1.2.0",
    "@octokit/auth-app": "^8.1.2",
    "@t3-oss/env-core": "^0.13.10",
    "@t3-oss/env-nextjs": "^0.13.10",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.42.0",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "hono": "^4.7.5",
    "octokit": "^5.0.5",
    "postgres": "^3.4.7",
    "stoker": "^2.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.5.1",
    "tsx": "^4.20.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1"
  },
  "packageManager": "pnpm@10.0.0"
