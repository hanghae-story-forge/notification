  "devDependencies": {
    "@types/node": "^22.14.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "tsx": "^4.20.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1"
